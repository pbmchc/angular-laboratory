{
  "name": "angular-laboratory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prepare": "husky install",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --configuration production --base-href /angular-laboratory/",
    "build:localize": "ng build --localize",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test --code-coverage",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "ng e2e",
    "extract-i18n": "ng extract-i18n --format json --output-path src/locale && node ./src/locale/postProcessMessages.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.3",
    "@angular/cdk": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/compiler": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/forms": "^17.3.3",
    "@angular/platform-browser": "^17.3.3",
    "@angular/platform-browser-dynamic": "^17.3.3",
    "@angular/router": "^17.3.3",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.3",
    "@angular/language-service": "^17.3.3",
    "@angular/localize": "^17.3.3",
    "@cypress/schematic": "2.5.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.19.29",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "cypress": "13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^9.0.11",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "~5.2.0"
  }
}