{
  "name": "angular-laboratory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prepare": "husky",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --configuration production --base-href /angular-laboratory/",
    "build:localize": "ng build --localize",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test --code-coverage",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "ng e2e",
    "extract-i18n": "ng extract-i18n --format json --output-path src/locale && node ./src/locale/postProcessMessages.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/platform-browser-dynamic": "^21.0.8",
    "@angular/router": "^21.0.8",
    "@fortawesome/angular-fontawesome": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2100.5",
    "@angular-devkit/core": "^21.0.5",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@angular/language-service": "^21.0.8",
    "@angular/localize": "^21.0.8",
    "@cypress/schematic": "^4.3.0",
    "@eslint/js": "^9.39.1",
    "@types/jasmine": "~5.1.13",
    "@types/node": "^22.19.3",
    "angular-eslint": "^21.1.0",
    "cypress": "^15.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
